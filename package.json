{
  "name": "interview-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.3.3",
    "axios": "^0.19.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.0.17"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "server": "cp -f server/db.json.orig server/db.json && yarn json-server -w -p 3001 server/db.json",
    "format": "prettier --ignore-path ./.gitignore --write \"src/**/*.{html,json,js,ts,css}\"",
    "format:check": "prettier --ignore-path ./.gitignore --check \"src/**/*.{html,json,js,ts,css}\"",
    "lint": "eslint --ignore-path ./.gitignore --fix --quiet \"src/**/*.{js,ts}\"",
    "lint:check": "eslint --ignore-path ./.gitignore \"src/**/*.{js,ts}\""
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "json-server": "^0.16.1",
    "prettier": "^2.0.5"
  }
}
